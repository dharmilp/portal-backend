<div class="wrapper ">
  <div class="sidebar" data-color="#2E4F9B" data-active-color="primary">
    <!--
      Tip 1: You can change the color of the sidebar using: data-color="blue | green | orange | red | yellow"
  -->
    <div class="logo">
      <div class="simple-text logo-normal" style="text-align: center">
          Welcome <%= session.userInfo.name %>
      </div>
    </div>
    <div class="sidebar-wrapper">
      <ul class="nav">
        <li >
          <a href="/dashboard">
            <i class="fas fa-tachometer-alt"></i>
            <p>Dashboard</p>
          </a>
        </li>
        <li class="active">
          <a href="#">
            <i class="fas fa-tasks"></i>
            <p>Quiz</p>
          </a>
        </li>
        <li >
          <a href="/users/uresult">
            <i class="fas fa-poll"></i>
            <p>Result</p>
          </a>
        </li>
        <li >
          <a href="/users/umyaccount">
            <i class="fas fa-user-circle"></i>
            <p>My account</p>
          </a>
        </li>
        <li>
          <a href="/users/logout">
            <i class="fa fa-sign-out" aria-hidden="true"></i>
            <p>Logout</p>
          </a>
        </li>
      </ul>
    </div>
  </div>
  <div class="main-panel">
    <!-- Navbar -->
    <nav class="navbar navbar-expand-lg navbar-absolute fixed-top navbar-transparent">
      <div class="container-fluid">
        <div class="navbar-wrapper">
          
          <a class="navbar-brand" href="#">Quiz</a>
        </div>
        <div class="collapse navbar-collapse justify-content-end" id="navigation">
        </div>
      </div>
    </nav>
    <div class="content">
        <div class="col-md-12">
            <div class="card">
              

              <div class="card-header">
                <h4 class="card-title"> Quizzes</h4>
              </div>
              <div class="card-body">
                <div class="table-responsive">
                  <table class="table">
                    <thead class=" text-primary">
                      <th>
                        Quiz Name
                      </th>
                      <th>
                        Start Date
                      </th>
                      <th>
                        Start Time
                      </th>
                      <th>
                        Duration
                      </th>
                      <th>
                        Action
                      </th>
                    </thead>
                    <tbody>
                        
                    <% for (var i = 0; i < quizzes.length; i++) { %>
                        <tr>
                            <td>
                                <%=quizzes[i].name%>
                            </td>
                            <td>
                              <%=quizzes[i].startDate.toDateString()%>
                            </td>
                            <td>
                              <%=quizzes[i].startDate.toTimeString().substr(0,8)%>
                            </td>
                            <td>
                              <%=quizzes[i].duration%>
                            </td>
                            <td>
                              <a href="/quiz/attempt/<%=quizzes[i]._id%>" class="btn btn-primary btn-round">Attempt</a>
                            </td>
                        </tr>
                    <% } %>        
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
            <% if (pages > 0) { %>
              <div class="pagination text-center">
                  <% if (current == 1) { %>
                      <a class="page-numbers">First</a>
                  <% } else { %>
                      <a class="page-numbers" href="/<%=docType%>?page=1">First</a>
                  <% } %>
                  <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
                  <% if (i !== 1) { %>
                      <a class="page-numbers">...</a>
                  <% } %>
                  <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
                      <% if (i == current) { %>
                          <a class="active page-numbers"><%= i %></a>
                      <% } else { %>
                          <a class="page-numbers" href="/<%=docType%>?page=<%= i %>"><%= i %></a>
                      <% } %>
                      <% if (i == Number(current) + 4 && i < pages) { %>
                          <a class="page-numbers">...</a>
                      <% } %>
                  <% } %>
                  <% if (current == pages) { %>
                      <a class="page-numbers">Last</a>
                  <% } else { %>
                      <a class="page-numbers" href="/<%=docType%>?page=<%= pages %>">Last</a>
                  <% } %>
              </div>
            <% } %>
          </div>
    </div>
  </div>
</div>

