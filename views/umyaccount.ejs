<div class="wrapper ">
  <div class="sidebar" data-color="#2E4F9B" data-active-color="primary">
    <!--
      Tip 1: You can change the color of the sidebar using: data-color="blue | green | orange | red | yellow"
  -->
    <div class="logo">
      <div class="simple-text logo-normal" style="text-align: center">
          Welcome <%= name %>
      </div>
    </div>
    <div class="sidebar-wrapper">
      <ul class="nav">
        <li >
          <a href="/dashboard">
            <i class="fas fa-tachometer-alt"></i>
            <p>Dashboard</p>
          </a>
        </li>
        <li >
          <a href="/users/uquiz?username=<%= name %>">
            <i class="fas fa-tasks"></i>
            <p>Quiz</p>
          </a>
        </li>
        <li >
          <a href="/users/uresult?username=<%= name %>">
            <i class="fas fa-poll"></i>
            <p>Result</p>
          </a>
        </li>
        <li class="active">
          <a href="#">
            <i class="fas fa-user-circle"></i>
            <p>My account</p>
          </a>
        </li>
        <li>
          <a href="/users/logout">
            <i class="fa fa-sign-out" aria-hidden="true"></i>
            <p>Logout</p>
          </a>
        </li>
      </ul>
    </div>
  </div>
  <div class="main-panel">
    <!-- Navbar -->
    <nav class="navbar navbar-expand-lg navbar-absolute fixed-top navbar-transparent">
      <div class="container-fluid">
        <div class="navbar-wrapper">
          
          <a class="navbar-brand" href="#">Account</a>
        </div>
        <div class="collapse navbar-collapse justify-content-end" id="navigation">
        </div>
      </div>
    </nav>
  </div>
</div>

