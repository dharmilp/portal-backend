            <i class="fa fa-sign-out" aria-hidden="true"></i>
            <p>Logout</p>
          </a>
        </li>
      </ul>
    </div>
  </div>
  <div class="main-panel">
    <!-- Navbar -->
    <nav class="navbar navbar-expand-lg navbar-absolute fixed-top navbar-transparent">
      <div class="container-fluid">
        <div class="navbar-wrapper">
          
          <a class="navbar-brand" href="/users/settings">Settings</a>
        </div>
        <div class="collapse navbar-collapse justify-content-end" id="navigation">
        </div>
      </div>
    </nav>
    <div class="content">
        <% include ./partials/messages %>
        <div class="col-md-12">
            <div class="card">
              <div class="card-header">
                <h4 class="card-title"> Groups</h4>
              </div>
              <div class="card-body">
                <div class="table-responsive">
                  <table class="table">
                    <thead class=" text-primary">
                      <th>
                        Name
                      </th>
                      <th></th>
                      <th></th>
                    </thead>
                    <tbody>
                        
                    <% for (var i = 0; i < groups.length; i++) { %>
                        <tr>
                            <td>
                                <%=groups[i].name%>
                            </td>
                            <td>
                              <a href="/groups/delete/<%=groups[i]._id%>?page=<%=current%>" class="btn btn-primary btn-round">DELETE</a>
                            </td>
                            <td>
                                <a href="/groups/groupEdit/<%=groups[i]._id%>?page=<%=current%>" class="btn btn-primary btn-round">EDIT</a>
                            </td>
                        </tr>
                    <% } %>        
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
            <% if (pages > 0) { %>
              <div class="pagination text-center">
                  <% if (current == 1) { %>
                      <a class="page-numbers">First</a>
                  <% } else { %>
                      <a class="page-numbers" href="/<%=docType%>?page=1">First</a>
                  <% } %>
                  <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
                  <% if (i !== 1) { %>
                      <a class="page-numbers">...</a>
                  <% } %>
                  <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
                      <% if (i == current) { %>
                          <a class="active page-numbers"><%= i %></a>
                      <% } else { %>
                          <a class="page-numbers" href="/<%=docType%>?page=<%= i %>"><%= i %></a>
                      <% } %>
                      <% if (i == Number(current) + 4 && i < pages) { %>
                          <a class="page-numbers">...</a>
                      <% } %>
                  <% } %>
                  <% if (current == pages) { %>
                      <a class="page-numbers">Last</a>
                  <% } else { %>
                      <a class="page-numbers" href="/<%=docType%>?page=<%= pages %>">Last</a>
                  <% } %>
              </div>
            <% } %>
          </div>
    </div>
  </div>
</div>

