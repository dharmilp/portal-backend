    <div class="main-panel">
      <!-- Navbar -->
      <nav class="navbar navbar-expand-lg navbar-absolute fixed-top navbar-transparent">
        <div class="container-fluid">
          <div class="navbar-wrapper">
            
            <a class="navbar-brand" href="#">Users</a>
          </div>
          <div class="collapse navbar-collapse justify-content-end" id="navigation">
          </div>
        </div>
      </nav>

      <div class="content">
        <!-- <a href="/users/addquestion" class="btn btn-primary btn-round">Add Question</a> -->
        <div class="col-md-12">
            <div class="card">
              <!-- <div class="card-header">
                  <a href="/users/signup" class="btn btn-primary btn-round">Add User</a>
              </div> -->
              <div class="card-body">
                <div class="table-responsive">
                  <table class="table">
                    <thead class=" text-primary">
                      <th>
                        Student ID
                      </th>
                      <th>
                        Name
                      </th>
                      <th>
                        Email
                      </th>
                      <th>
                        Action
                      </th>
                    </thead>
                    <tbody>
                        
                        <% for (var i = 0; i < users.length; i++) { %>
                          <tr>
                              <td>
                                  <%-users[i].studentId%>
                              </td>
                              <td>
                                  <%=users[i].name%>
                              </td>
                              <td>
                                  <%=users[i].email%>
                              </td>
                              <td>
                                <a href="/users/usersdelete/<%=users[i]._id%>?page=<%=current%>" class="btn btn-primary btn-round">Delete</a>
                              </td>
                          </tr>
                      <% } %>
        
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
            <% if (pages > 0) { %>
              <div class="pagination text-center">
                  <% if (current == 1) { %>
                      <a class="page-numbers">First</a>
                  <% } else { %>
                      <a class="page-numbers" href="/<%=docType%>?page=1">First</a>
                  <% } %>
                  <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
                  <% if (i !== 1) { %>
                      <a class="page-numbers">...</a>
                  <% } %>
                  <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
                      <% if (i == current) { %>
                          <a class="active page-numbers"><%= i %></a>
                      <% } else { %>
                          <a class="page-numbers" href="/<%=docType%>?page=<%= i %>"><%= i %></a>
                      <% } %>
                      <% if (i == Number(current) + 4 && i < pages) { %>
                          <a class="page-numbers">...</a>
                      <% } %>
                  <% } %>
                  <% if (current == pages) { %>
                      <a class="page-numbers">Last</a>
                  <% } else { %>
                      <a class="page-numbers" href="/<%=docType%>?page=<%= pages %>">Last</a>
                  <% } %>
              </div>
            <% } %>
          </div>  
      </div>

    </div>
  </div>
  
